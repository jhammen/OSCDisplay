<resources>
    <string name="app_name">OSCDisplay</string>
    <string name="default_template" formatted="false">
        <![CDATA[<html>
            <body><b>{$path}</b><br />
                received {$time}<br/>
                {.if ($numargs) } arguments: {/if}
                <ul>{% loop in $args as $arg  %}
                    <li>{$arg}</li>
                {% onEmpty %}
                 (No arguments)
                {% endloop %}</ul>
            </body>
        </html>]]>
    </string>
    <string name="template_waiting" formatted="false">
        <![CDATA[
            <html>
            <head>
            <style type="text/css">
            #text{
            font-size:28;
            text-align: center;
            line-height: normal;
            color:#BBBBBB;
            margin:auto;
            }
            #top {
            height: 33%
            }
            </style>
            </head>
            <body>
            <div id="top">&nbsp;</div>
            <p id="text">Waiting for OSC message</p>
            </body>
            </html>
        ]]>
    </string>
